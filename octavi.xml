<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- Github -->
  <remote  name="github"
           fetch="https://github.com/" />

  <!-- GitHub (Project-Awaken) -->
  <remote  name="octavi"
           fetch="https://github.com/Octavi-Testing-Org"
           revision="12.1" />

  <!-- GitLab -->
  <remote  name="gitlab"
           revision="12.1"
           fetch="https://gitlab.com/Project-Awaken/" />

  <!--ART-->
  <project path="art" name="android_art" groups="pdk" remote="octavi" />

  <!-- Rootdir -->
  <project path="bionic" name="android_bionic" groups="pdk" remote="octavi" />
  <project path="libcore" name="android_libcore" groups="pdk" remote="octavi" />
  <project path="manifest" name="android_manifest" remote="octavi" />

  <!-- Bootable -->
  <project path="bootable/recovery" name="android_bootable_recovery" groups="pdk" remote="octavi" />

  <!-- Build -->
  <project path="build/make" name="android_build" groups="pdk" remote="octavi" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="build/soong" name="android_build_soong" groups="pdk,tradefed" remote="octavi" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>

  <!-- Device -->
  <project path="device/octavi/sepolicy" name="android_device_octavi_sepolicy" remote="octavi" />
  <project path="device/qcom/sepolicy" name="android_device_qcom_sepolicy" groups="qcom,pdk-qcom" remote="octavi" />
  <project path="device/qcom/sepolicy-legacy" name="android_device_qcom_sepolicy" groups="qcom,pdk-qcom" revision="12.1-legacy" remote="octavi" />
  <project path="device/qcom/sepolicy-legacy-um" name="android_device_qcom_sepolicy" groups="qcom,pdk-qcom" revision="12.1-legacy-um" remote="octavi" />
  <project path="device/qcom/sepolicy_vndr" name="android_device_qcom_sepolicy_vndr" groups="qcom,pdk-qcom" remote="octavi" />

  <!-- External -->
  <project path="external/tinyxml" name="platform/external/tinyxml" groups="pdk" remote="aosp" revision="refs/tags/android-11.0.0_r46" />

  <!-- Frameworks -->
  <project path="frameworks/av" name="android_frameworks_av" remote="octavi" />
  <project path="frameworks/base" name="android_frameworks_base" groups="pdk-cw-fs,pdk-fs" remote="octavi" />
  <project path="frameworks/native" name="android_frameworks_native" groups="pdk-cw-fs,pdk-fs" remote="octavi" />

  <!-- Hardware -->
  <project path="hardware/lineage/interfaces" name="android_hardware_lineage_interfaces" remote="octavi" />

  <!-- Packages -->
  <project path="packages/apps/Dialer" name="android_packages_apps_Dialer" remote="octavi" />
  <project path="packages/apps/Settings" name="android_packages_apps_Settings" remote="octavi" />
  <project path="packages/apps/SettingsIntelligence" name="android_packages_apps_SettingsIntelligence" remote="octavi" />
  <project path="packages/apps/SimpleDeviceConfig" name="android_packages_apps_SimpleDeviceConfig" remote="octavi" />
  <project path="packages/apps/Launcher3" name="android_packages_apps_Launcher3" remote="octavi" />
  <project path="packages/apps/ThemePicker" name="android_packages_apps_ThemePicker" remote="octavi" />
  <project path="packages/apps/TouchGestures" name="android_packages_apps_TouchGestures" remote="octavi" />
  <project path="packages/apps/Updater" name="android_packages_apps_Updater" remote="octavi" />
  <project path="packages/apps/WallpaperPicker2" name="android_packages_apps_WallpaperPicker2" remote="octavi" />
  <project path="packages/modules/Connectivity" name="android_packages_modules_Connectivity" groups="pdk" remote="octavi" />
  <project path="packages/modules/Permission" name="android_packages_modules_Permission" remote="octavi" />
  <project path="packages/modules/Wifi" name="android_packages_modules_Wifi" remote="octavi" />
  <project path="packages/providers/DownloadProvider" name="android_packages_providers_DownloadProvider" remote="octavi" />
  <project path="packages/services/Telecomm" name="android_packages_services_Telecomm" remote="octavi" />

  <!-- System -->
  <project path="system/core" name="android_system_core" groups="pdk" remote="octavi" />
  <project path="system/sepolicy" name="android_system_sepolicy" remote="octavi" />
  <project path="system/update_engine" name="android_system_update_engine" remote="octavi" />

  <!-- Vendor -->
  <project path="vendor/octavi" name="android_vendor_octavi" remote="octavi" />
  <project path="vendor/support" name="android_vendor_support" remote="octavi" />
  <project path="vendor/qcom/opensource/interfaces" name="android_vendor_qcom_opensource_interfaces" groups="qcom,pdk-qcom" remote="octavi" />
  <project path="vendor/gms" name="android_vendor_gms" remote="gitlab" clone-depth="1" />

